{
  "name": "Jugaadu GCR",
  "description": "A Chrome Extension to make Google Classroom better",
  "manifest_version": 3,
  "version": "1",
  "host_permissions": ["*://classroom.google.com/"],
  "content_scripts": [
    {
      "matches": ["*://classroom.google.com/*"],
      "js": ["content.js"]
    }
  ]
}